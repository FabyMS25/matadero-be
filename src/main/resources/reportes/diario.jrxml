	<queryString language="SQL">
		<![CDATA[SELECT tt.concepto, sum(cantidad) as cantidad , tt.pre as precio_cobrado, SUM(total) AS importe_total
       FROM
		((SELECT 'PORCINO' AS concepto, sum(cantidad) as cantidad, abs(precio) as pre , SUM(total) AS total
        FROM orden ord
		WHERE  ord.id_categoria=1 and ord.fecha =$P{dia}
		 group by precio)
		UNION ALL
		(SELECT 'OVINO' AS concepto, sum(cantidad) as cantidad, abs(precio) as pre,SUM(total) AS total
        FROM orden ord
		WHERE  ord.id_categoria=2  and ord.fecha =$P{dia} 
		 group by precio)
        UNION ALL 
        (SELECT 'BOVINO' AS concepto, sum(cantidad) as cantidad,abs(precio) as pre, SUM(total) AS total
        FROM orden ord
		WHERE  ord.id_categoria=3  and ord.fecha =$P{dia}
		group by precio)) AS tt
		GROUP by concepto,pre]]>
	</queryString>
